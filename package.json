{
  "name": "render",
  "version": "1.0.0",
  "description": "Opinionated React template",
  "author": "Omar Elhawary <oedotme@gmail.com>",
  "license": "MIT",
  "keywords": [
    "app",
    "boilerplate",
    "client",
    "context",
    "data-loaders",
    "file-based-routing",
    "frontend",
    "hooks",
    "nested-layouts",
    "react",
    "react-location",
    "routes",
    "structure",
    "tanstack",
    "template",
    "vite",
    "web"
  ],
  "scripts": {
    "start": "vite dev --port 3000",
    "dev": "vite dev --port 3000",
    "build": "npm-run-all type-check client",
    "export": "npm-run-all static server type-check prerender",
    "preview": "vite preview build/client --port 5000",
    "serve:client": "serve build/client/dist",
    "serve:static": "serve build/static/dist",
    "client": "vite build --outDir build/client/dist",
    "static": "vite build --outDir build/static/dist",
    "server": "vite build --outDir build/server/dist --ssr src/server.tsx",
    "prerender": "esbuild ./prerender.ts | node --input-type=module",
    "type-check": "tsc",
    "format": "prettier --write 'src/**/*.{css,html,json,md,mdx,js,jsx,ts,tsx}'",
    "lint": "eslint --fix 'src/**/*.{js,jsx,ts,tsx}'",
    "prepare": "husky install",
    "test": "DOTENV_CONFIG_PATH=.env jest",
    "test:debug": "DOTENV_CONFIG_PATH=.env node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --no-cache",
    "sample": "echo 'ENG-123: badtag(api)!: jira lint' | commitlint"
  },
  "dependencies": {
    "@emotion/styled": "^11.10.5",
    "@hookform/resolvers": "^2.9.10",
    "@mui/icons-material": "^5.10.14",
    "@mui/lab": "^5.0.0-alpha.108",
    "@mui/material": "^5.10.14",
    "@mui/styled-engine": "^5.10.14",
    "@mui/x-date-pickers": "^5.0.8",
    "@tanstack/react-location": "^3.7.4",
    "date-fns": "^2.29.3",
    "generouted": "^1.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.39.5",
    "react-hook-form-mui": "^5.10.2",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-pipeline-operator": "^7.18.9",
    "@commitlint/cli": "^17.2.0",
    "@commitlint/config-conventional": "^17.2.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "@vitejs/plugin-react": "^2.2.0",
    "autoprefixer": "^10.4.13",
    "babel-preset-vite": "^1.0.4",
    "dotenv": "^16.0.3",
    "esbuild": "^0.15.13",
    "esbuild-register": "^3.4.1",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "husky": "^8.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "jest-transform-stub": "^2.0.0",
    "lint-staged": "^13.0.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.18",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "serve": "^14.0.1",
    "tailwindcss": "^3.2.2",
    "ts-jest": "^29.0.3",
    "typescript": "^4.8.4",
    "vite": "^3.2.4"
  }
}
